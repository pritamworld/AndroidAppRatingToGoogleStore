<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <solid android:color="@color/love_button_no_start_color"/>
            <stroke android:width="1dp" android:color="@color/love_button_no_pressed_color" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <stroke android:width="1dp" android:color="@color/love_button_no_start_color" />
            <solid android:color="@color/love_button_no_start_color"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle">
            <corners android:radius="4dp"/>
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
            <solid android:color="@color/love_button_no_start_color"/>
            <stroke android:width="1dp" android:color="@color/love_button_no_start_color" />
        </shape>
    </item>
</selector>